<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleProgressView">
        <!--半径-->
        <attr name="radius" format="dimension"/>
        <!--色带宽度-->
        <attr name="stripeWidth" format="dimension"/>
        <!--进度-->
        <attr name="progress" format="integer"/>
        <!--色带颜色-->
        <attr name="progressColor" format="color"/>
        <!--外圈颜色-->
        <attr name="backgroundProgressColor" format="color"/>
        <attr name="backgroundInColor" format="color"/>
        <!--中间字体颜色-->
        <attr name="textColorOne" format="color"/>
        <attr name="textColorTwo" format="color"/>
        <attr name="textColorThree" format="color"/>
        <!--中间字体大小-->
        <attr name="textSizeOne" format="dimension"/>
        <attr name="textSizeTwo" format="dimension"/>
        <attr name="textSizeThree" format="dimension"/>
        <!--中间字体加粗-->
        <attr name="textBoldOne" format="boolean"/>
        <attr name="textBoldTwo" format="boolean"/>
        <attr name="textBoldThree" format="boolean"/>
        <!--中间字体倾斜-->
        <!--方向-->
        <attr name="isClockwise" format="boolean"/>
    </declare-styleable>

</resources>